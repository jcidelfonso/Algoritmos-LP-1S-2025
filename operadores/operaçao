#include <stdio.h>

int main() {

int n1=0, n2=0,soma=0,sub=0,mult=0,div=0;

printf("---CALCULA TUDO---\n");

printf("Informe N1: ");
scanf("%d",&n1);

printf("Informe N2: ");
scanf("%d",&n2);

soma = n1+ n2;
sub = n1 - n2;
mult = n1 * n2;
div = n1 / n2;

printf("Resultado da soma: %d\n", soma);
printf("Resultado da subtracao: %d\n", sub);
printf("Resultado da multiplicao: %d\n", mult);
printf("Resultado da divisao: %d\n", div);

return 0;
}